<div>
  <h1>Question</h1>
  {[{exerciseData.subject}]}
</div>
<div>
  <form>
    <p ng-repeat="answer in exerciseData.answers">
      <input type="radio" name="answer" ng-click="oneAnswer($index)"/> {[{answer}]}
    </p>
  </form>
  <p>{[{answerValue}]}</p>
  <button ng-click="checkAnswers(answers)">v√©rifier</button>
  <form action="http://google.com">
    <input ng-disabled="!isRightAnswer" type="submit" value="Go to Google">
  </form>
</div>
